{
  "version": 2,
  "name": "coopsaving-dapp",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_SEPOLIA_RPC_URL": "@vite_sepolia_rpc_url",
    "VITE_CONTRACT_ADDRESS": "@vite_contract_address"
  }
}